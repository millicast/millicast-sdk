{
  "name": "@nx-millicast/source",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "./check-signature.sh"
    }
  },
  "lint-staged": {
    "*": [
      "npm run lint"
    ]
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "nx build @millicast/sdk",
    "build-docs": "nx build-docs @millicast/sdk",
    "pretty": "npm run pretty --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "lint-fix": "npm run lint-fix --workspaces --if-present",
    "start": "nx run-many -t serve --parallel --maxParallel=100",
    "start-docs": "nx start-docs @millicast/sdk",
    "test": "nx test @millicast/sdk",
    "test-e2e": "nx run-many --target=test-e2e --all --parallel --output-style=stream"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/helpers": "^7.13.10",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.23.6",
    "@babel/runtime": "^7.13.10",
    "@maslick/radiaslider": "^1.9.8",
    "@nx/jest": "^21.3.2",
    "@nx/js": "^21.3.2",
    "@nx/vite": "^21.3.2",
    "@nx/web": "^21.3.2",
    "@nx/workspace": "^21.3.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@types/node": "^24.0.12",
    "@vitest/ui": "^3.2.4",
    "babel-jest": "^29.7.0",
    "core-js": "^3.20.1",
    "dotenv": "^17.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-n": "^17.21.0",
    "husky": "^4.3.8",
    "jsdom": "~26.1.0",
    "netlify-cli": "^22.3.0",
    "npm": "^11.4.2",
    "nx": "^21.3.2",
    "prettier": "^3.6.2",
    "puppeteer": "^24.12.1",
    "serve": "^14.2.4",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.2",
    "typedoc": "^0.28.5",
    "typedoc-plugin-mdn-links": "^5.0.2",
    "typescript": "^5.8.3",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.0.0"
  }
}
